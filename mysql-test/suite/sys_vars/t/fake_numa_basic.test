--source include/load_sysvars.inc

SELECT @@GLOBAL.fake_numa;

--error ER_INCORRECT_GLOBAL_LOCAL_VAR
SELECT @@SESSION.fake_numa;

SELECT IF(@@global.fake_numa, "ON", "OFF") = VARIABLE_VALUE 
FROM INFORMATION_SCHEMA.GLOBAL_VARIABLES 
WHERE VARIABLE_NAME='fake_numa';

SELECT IF(@@fake_numa, "ON", "OFF") = VARIABLE_VALUE 
FROM INFORMATION_SCHEMA.SESSION_VARIABLES 
WHERE VARIABLE_NAME='fake_numa';

SELECT @@fake_numa = @@global.fake_numa;
